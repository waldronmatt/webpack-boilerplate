{
  "name": "webpack-template",
  "description": "Webpack 5 template with sensible defaults.",
  "main": "src/js/index.js",
  "scripts": {
    "dev": "webpack serve --env development --config config/webpack.dev.js",
    "prod": "webpack --env production --config config/webpack.prod.js && webpack --env target=server --config config/webpack.server.config.js && node ./dist/server.js",
    "build": "webpack --env production --config config/webpack.prod.js && webpack --env target=serverless --config config/webpack.server.config.js",
    "lint:check": "eslint .",
    "lint:fix": "eslint --fix ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/waldronmatt/webpack-template.git"
  },
  "author": "Matthew Waldron",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/waldronmatt/webpack-template/issues"
  },
  "homepage": "https://github.com/waldronmatt/webpack-template#readme",
  "browserslist": [
    "extends @waldronmatt/browserslist-config"
  ],
  "dependencies": {
    "express": "^4.17.2",
    "express-rate-limit": "^6.2.1",
    "express-static-gzip": "^2.1.5",
    "sanitize.css": "^13.0.0",
    "serverless-http": "^2.7.0",
    "the-new-css-reset": "^1.4.9"
  },
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.1",
    "@waldronmatt/browserslist-config": "^1.0.2",
    "@waldronmatt/eslint-config": "^1.0.7",
    "@waldronmatt/postcss-config": "^1.0.0",
    "@waldronmatt/prettier-config": "^1.1.4",
    "@waldronmatt/stylelint-config": "^1.0.9",
    "@waldronmatt/webpack-config": "^2.5.0",
    "compression-webpack-plugin": "^9.2.0",
    "eslint": "^8.9.0",
    "eslint-webpack-plugin": "^3.1.1",
    "html-webpack-plugin": "^5.5.0",
    "postcss": "^8.4.6",
    "prettier": "^2.5.1",
    "stylelint": "^14.5.0",
    "stylelint-webpack-plugin": "^3.1.1",
    "webpack": "^5.68.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4",
    "webpack-merge": "^5.8.0",
    "webpack-node-externals": "^3.0.0"
  }
}
